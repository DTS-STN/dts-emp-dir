{
    "name": "testing",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start-localdb": "yarn --cwd ./_server/ start-localdb",
        "start-client": "yarn --cwd ./_client/ start",
        "start-server": "yarn --cwd ./_server/ start",
        "start-unit-test": "yarn --cwd ./_client/ test"
    },
    "husky": {
        "hooks": {
          "pre-commit": "yarn run lint && yarn test",
          "pre-push": "yarn run lint && yarn test"
        }
      },
    "dependencies": {
        "dotenv": "^8.2.0"
    },
    "devDependencies": {
        "husky": "^4.2.3",
        "cross-env": "^7.0.2"
      }
}
